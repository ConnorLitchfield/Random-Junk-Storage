<!doctype html>
<html>
   <HTA:APPLICATION
      SINGLEINSTANCE="yes"
      BORDER="thick"
      BORDERSTYLE="normal"
      CONTEXTMENU="yes"
      INNERBORDER="yes"
	  />
<head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<title>Send Keys Basic</title>
</head>
<script type="text/vbscript">
window.ResizeTo 1140, 590

Public Sub setall()
where1.innerhtml = user1.value
where2.innerhtml = user2.value
where3.innerhtml = user3.value
where4.innerhtml = user4.value
where5.innerhtml = user5.value
where6.innerhtml = user6.value
where7.innerhtml = user7.value
where8.innerhtml = user8.value
where9.innerhtml = user9.value
where10.innerhtml = user10.value
app1.innerhtml = userapp.value & "   "
End Sub

Public Sub sendsingle(varval)
Dim WShell : Set WShell = CreateObject("WScript.Shell")
With WShell
.AppActivate userapp.value
if varval="1" then
.SendKeys user1.value
elseif varval="2" then
.SendKeys user2.value
elseif varval="3" then
.SendKeys user3.value
elseif varval="4" then
.SendKeys user4.value
elseif varval="5" then
.SendKeys user5.value
elseif varval="6" then
.SendKeys user6.value
elseif varval="7" then
.SendKeys user7.value
elseif varval="8" then
.SendKeys user8.value
elseif varval="9" then
.SendKeys user9.value
elseif varval="10" then
.SendKeys user10.value
End if
End With
End Sub

Public Sub sendall()
Dim WShell : Set WShell = CreateObject("WScript.Shell")
With WShell
.AppActivate userapp.value
.SendKeys user1.value & user2.value & user3.value & user4.value & user5.value & user6.value & user7.value & user8.value & user9.value & user10.value
End With
End Sub

</script>
<body background="back.jpg">

 <p style="float: right;"><img src="hints.jpg" height="510" width="400" border="1px"></p>

<input type="button" value="Set all values" onClick="setall">
<input type="button" value="Send all values" onClick="sendall">
AppActive: <span id="app1" style="font-weight:800;">  </span><input type="text" name="userapp" size="10"><BR><BR>


<input type="button" value="Send 1" onClick="sendsingle(1)">
<input type="button" value="Send 2" onClick="sendsingle(2)">
<input type="button" value="Send 3" onClick="sendsingle(3)">
<input type="button" value="Send 4" onClick="sendsingle(4)">
