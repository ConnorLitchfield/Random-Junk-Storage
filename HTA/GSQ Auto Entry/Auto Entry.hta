        <button onclick="SAP2('c9e39725dc335a3fcb6ff29acf165b86')">Strategic Sourcing</button>
        <button onclick="SAP2('887fe5aae4a0738db06ed19a9794f097')">Universal Worklist</button><BR>
        <button onclick="FlexSheet()">Flex Sheet</button>
