<script type="text/vbscript">
      Public Sub Window_onLoad
	  window.resizeTo 1080,670
    End Sub

Sub capture
Set WshShell = CreateObject("WScript.Shell")
Connect="powershell -windowstyle hidden -ExecutionPolicy Bypass -File shells/capture6.ps1"
WshShell.Run Connect, 4, false
End Sub


Sub delimg
Set WshShell = CreateObject("WScript.Shell")
Connect="powershell -windowstyle hidden -ExecutionPolicy Bypass -File shells/delimg.ps1"
WshShell.Run Connect, 4, false
End Sub

</script>

