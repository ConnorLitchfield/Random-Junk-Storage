<!doctype html>
<html>
   <HTA:APPLICATION
	  ICON="icon.ico"
      BORDER="thick"
      BORDERSTYLE="normal"
      CONTEXTMENU="yes"
      INNERBORDER="yes"
	  />
<head>
<meta http-equiv="content-type" content="text/html; charset=UTF-8">
<title>Match Helper Level 4</title>
</head>
<script type="text/vbscript">
      Public Sub Window_onLoad
	  window.resizeTo 470,560
    End Sub

Sub capture
Set WshShell = CreateObject("WScript.Shell")
Connect="powershell -windowstyle hidden -ExecutionPolicy Bypass -File shells/capture4.ps1"
WshShell.Run Connect, 4, false
End Sub


Sub delimg
Set WshShell = CreateObject("WScript.Shell")
Connect="powershell -windowstyle hidden -ExecutionPolicy Bypass -File shells/delimg.ps1"
WshShell.Run Connect, 4, false
End Sub

</script>

<body background="1.jpg">
 <input type="button" value="Delete All Images" onClick="delimg">
 <br>
 <input type="button" value="Capture All Lines" onClick="capture">
 <br>
  <br>
<div class=”gallery”>
